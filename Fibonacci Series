import java.util.Scanner;
class main
{
    public static void main(String[] args) {
        Scanner z=new Scanner(System.in);
        int n,a=0,b=1,c=0;
        n=z.nextInt();
        
       for(int i=2;i<=n;i++)
       {
           c=a+b;
           a=b;
           b=c;
       }
        System.out.print(c);   
    }
}
